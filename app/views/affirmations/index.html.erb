<h1>Affirmations</h1>
 


<table>
<tr> 
  <% @affirmations.each do |affirmation| %>
      <td><%= affirmation.text %></td>
      <td><button><%= link_to 'New', affirmation_new_path %></button></td>
      <td><%= link_to 'Show', affirmation_show_path %></td>
      <td><button type="button" class="btn btn-primary"><%= link_to 'New Affirmation', new_affirmation_path %></button></td>
    </tr>
  <% end %>
</table>

